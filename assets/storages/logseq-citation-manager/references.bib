@article{hadjihambiHepaticEncephalopathyCritical2018,
  title = {Hepatic Encephalopathy: A Critical Current Review},
  shorttitle = {Hepatic Encephalopathy},
  author = {Hadjihambi, Anna and Arias, Natalia and Sheikh, Mohammed and Jalan, Rajiv},
  date = {2018-02-01},
  journaltitle = {Hepatology International},
