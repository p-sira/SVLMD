@article{acharyaCurrentManagementHepatic2018,
  title = {Current {{Management}} of {{Hepatic Encephalopathy}}},
  author = {Acharya, Chathur and Bajaj, Jasmohan S.},
  date = {2018-11},
  journaltitle = {Official journal of the American College of Gastroenterology | ACG},
  volume = {113},
  number = {11},
  pages = {1600},
  issn = {0002-9270},
  doi = {10.1038/s41395-018-0179-4},
  url = {https://journals.lww.com/ajg/fulltext/2018/11000/current_management_of_hepatic_encephalopathy.15.aspx},
  urldate = {2025-06-11},
  abstract = {Hepatic encephalopathy is a state of brain dysfunction resulting from decompensation of cirrhosis. The mortality and morbidity associated with the overt form of hepatic encephalopathy are high, and even the covert form associates with poor outcomes and poor quality of life. We know that the dysfunction is not just an acute insult to the brain but rather results in long-standing cognitive issues that get worse with each episode of HE. Hence, there is an urgency to accurately diagnose these conditions, start appropriate therapy, and to maintain remission. Currently, we have two mainstay pharmacological treatment options (lactulose and rifaximin), but the narrative is evolving with new therapies under trial. Microbiome manipulation resulting in a favorable change to the gut microbiota seems to be a promising new area of therapy.},
  langid = {american},
  file = {/home/psira/Zotero/storage/KDU58AHT/current_management_of_hepatic_encephalopathy.15.html},
  year = {2018}
}

