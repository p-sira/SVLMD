  title = {{{ACUTE}} ({{ASCENDING}}) {{CHOLANGITIS}}},
  author = {Hanau, Lawrence H. and Steigbigel, Neal H.},
  date = {2000-09-01},
  journaltitle = {Infectious Disease Clinics of North America},
  shortjournal = {Infectious Disease Clinics of North America},
