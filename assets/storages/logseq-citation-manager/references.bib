@article{williamsUpdatedGuidelineManagement2017,
  title = {Updated Guideline on the Management of Common Bile Duct Stones ({{CBDS}})},
  author = {Williams, Earl and Beckingham, Ian and Sayed, Ghassan El and Gurusamy, Kurinchi and Sturgess, Richard and Webster, George and Young, Tudor},
